{
  "name": "maki-partner-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Frontend moderno para MakiPartner - Sistema de gesti√≥n empresarial basado en Odoo",
  "keywords": [
    "react",
    "typescript",
    "odoo",
    "erp",
    "heroui",
    "vite"
  ],
  "author": "MakiPartner Team",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmitOnError && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@heroui/react": "2.7.9",
    "@heroui/use-theme": "2.1.6",
    "@iconify/react": "latest",
    "axios": "^1.6.2",
    "framer-motion": "^11.18.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "5.3.4",
    "recharts": "^2.15.3",
    "source-map-js": "^1.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/generator": "^7.27.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.20",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "8.4.49",
    "prettier": "^3.1.0",
    "tailwindcss": "3.4.17",
    "terser": "^5.24.0",
    "ts-jest": "^29.1.1",
    "typescript": "5.7.3",
    "vite": "^6.0.11",
    "vite-bundle-analyzer": "^0.7.0"
  }
}
